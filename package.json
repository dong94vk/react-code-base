{
  "name": "boxsocial",
  "version": "1.0.1",
  "description": "Boxsocial system build on React,TypeScript and Webpack",
  "scripts": {
    "start": "webpack-dev-server --config webpack/run.config.js --env=dev --watch --watch-poll",
    "build": "webpack --config webpack/run.config.js --env=prod",
    "stg": "webpack --config webpack/run.config.js --env=stg",
    "bbtest": "webpack --config webpack/run.config.js --env=test",
    "test": "jest --colors --config ./jest/jest.config.js",
    "tslint": "tslint 'src/**/*.*.{ts,tsx}'",
    "tslint:fix": "tslint --fix 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && bash ./pre-commit.sh",
      "pre-push": "npm run tslint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "npm run tslint:fix",
      "git add"
    ]
  },
  "author": "Box-Developer",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.0",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "lodash": "4.17.20",
    "qs": "6.5.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-gateway": "3.0.0",
    "react-i18next": "^11.10.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.1.2",
    "react-scroll": "1.8.1",
    "redux": "4.0.5",
    "redux-form": "8.3.5",
    "redux-logger": "3.0.6",
    "redux-persist": "5.9.1",
    "redux-thunk": "2.3.0"
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-redux": "7.1.12"
  },
  "devDependencies": {
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.109",
    "@types/node": "11.11.6",
    "@types/qs": "6.5.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router-dom": "5.1.2",
    "@types/redux-form": "8.2.5",
    "@types/redux-thunk": "2.1.0",
    "autoprefixer": "7.1.6",
    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "2.0.1",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "5.1.0",
    "css-loader": "0.28.4",
    "dotenv-webpack": "1.5.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.3.4",
    "file-loader": "0.11.2",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "1.3.1",
    "jest": "23.0.1",
    "jest-enzyme": "6.0.1",
    "lint-staged": "8.1.5",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "2.0.8",
    "prettier": "2.0.5",
    "react-test-renderer": "16.4.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.18.2",
    "ts-jest": "22.4.6",
    "ts-node": "6.0.5",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.9.2",
    "typescript": "3.9.5",
    "url-loader": "1.1.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  }
}
